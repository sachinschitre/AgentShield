{
  "name": "agentshield-server",
  "version": "1.0.0",
  "description": "AgentShield Backend API",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cloudflare": "node -e \"require('./services/CloudflareAgentTester').test()\"",
    "test:wrangler": "wrangler dev --test",
    "test:security": "npm run test:cloudflare && npm run test:wrangler",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "@cloudflare/agent-sdk": "^1.0.0",
    "wrangler": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  }
}
