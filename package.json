{
  "name": "agentshield",
  "version": "1.0.0",
  "description": "Developer tool for detecting and testing loopholes in agentic AI applications",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "cd server && npm test && cd ../client && npm test",
    "test:integration": "node tests/integration-tests.js",
    "test:post-deploy": "node tests/post-deployment-tests.js",
    "lint": "cd server && npm run lint && cd ../client && npm run lint",
    "format": "cd server && npm run format && cd ../client && npm run format",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["ai", "security", "testing", "agentic", "vulnerability"],
  "author": "AgentShield Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "axios": "^1.6.2",
    "wait-on": "^7.2.0"
  }
}
